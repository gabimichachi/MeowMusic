CREATE TABLE MUSICA (
 CODIGO INT NOT NULL,
 CANTOR VARCHAR(50),
 GENERO CHAR(50),
 DURACAO TIME(10),
 NOME VARCHAR(50),
 URL_IMAGEM VARCHAR(255)
);

ALTER TABLE MUSICA ADD CONSTRAINT PK_MUSICA PRIMARY KEY (CODIGO);


CREATE TABLE GENERO (
 NOME VARCHAR(30) NOT NULL,
 ICONE CHAR(100),
 COR VARCHAR(10),
 CODIGO INT
);

ALTER TABLE GENERO ADD CONSTRAINT PK_GENERO PRIMARY KEY (NOME);


ALTER TABLE GENERO ADD CONSTRAINT FK_GENERO_0 FOREIGN KEY (CODIGO) REFERENCES MUSICA (CODIGO);


